{
  "name": "iamgld-ui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "start:open": "ng serve --configuration development --open",
    "start:host": "ng serve --configuration development --host 0.0.0.0",
    "start:stag": "ng serve --configuration staging",
    "start:prod": "ng serve --configuration production",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run build:prod",
    "build:dev": "ng build --configuration development --progress",
    "build:stag": "ng build --configuration staging --progress",
    "build:prod": "ng build --configuration production --progress",
    "server:ssr": "node dist/server/server.mjs",
    "serve:ssr:iamgld-ui-angular": "node dist/iamgld-ui-angular/server/server.mjs",
    "prettier:": "prettier \"src/**/*.{ts,html,scss,css,js,json,md}\" --check --config .prettierrc",
    "prettier:fix": "prettier \"src/**/*.{ts,html,scss,css,js,json,md}\" --write --config .prettierrc",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --fix src/**/*.{css,scss}",
    "eslint": "ng lint",
    "eslint:fix": "ng lint --fix",
    "linter": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
    "lintstaged": "npx lint-staged",
    "postinstall": "npx husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.1",
    "@angular/common": "19.0.1",
    "@angular/compiler": "19.0.1",
    "@angular/core": "19.0.1",
    "@angular/forms": "19.0.1",
    "@angular/platform-browser": "19.0.1",
    "@angular/platform-browser-dynamic": "19.0.1",
    "@angular/platform-server": "19.0.1",
    "@angular/router": "19.0.1",
    "@angular/ssr": "19.0.2",
    "@ngrx/signals": "18.1.1",
    "express": "4.21.1",
    "rxjs": "7.8.1",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.2",
    "@angular/cli": "19.0.2",
    "@angular/compiler-cli": "19.0.1",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@schematics/angular": "19.0.2",
    "@types/express": "5.0.0",
    "@types/node": "22.10.1",
    "angular-eslint": "19.0.0",
    "eslint": "9.16.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.4.1",
    "stylelint": "16.11.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-selector-bem-pattern": "4.0.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.16.0"
  }
}

